<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Login" %>
<% end %>
Not a registered user? SignUpp to <%= link_to "Create an Account", new_user_path %>

<%= flash[:notice] %>
